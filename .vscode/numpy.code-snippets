      "def produit_scalaire(tableau_A, tableau_B):",
      "    if tableau_A.shape != tableau_B.shape:",
      "        raise ValueError('Les deux tableaux doivent avoir la même dimension.')",
      "    return np.dot(tableau_A, tableau_B)",
      "# A = np.array([1, 2, 3])",
      "# B = np.array([4, 5, 6])",
      "# produit = produit_scalaire(A, B)  # 32"
