  "numpy_binariser": {
    "prefix": "numpy_binariser",
    "body": [
      "def binariser_tableau(tableau, seuil):",
      "    resultat = np.zeros_like(tableau)",
      "    resultat[tableau >= seuil] = 1",
      "    return resultat",
      "# tableau = np.array([2, 5, 7, 3])",
      "# binaire = binariser_tableau(tableau, 5)  # [0 1 1 0]"
    ],
    "description": "Binarise un tableau selon un seuil donné"
